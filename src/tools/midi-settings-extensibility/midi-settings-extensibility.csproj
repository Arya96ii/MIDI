<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup Label="Globals">
	  <WebView2UseWinRT>False</WebView2UseWinRT>
	</PropertyGroup>

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<TargetFramework>net8.0-windows10.0.20348.0</TargetFramework>
		<TargetPlatformMinVersion>10.0.20348.0</TargetPlatformMinVersion>
		<RootNamespace>Microsoft.Midi.Settings.Extensibility</RootNamespace>
		<AssemblyName>Microsoft.Midi.Settings.Extensibility</AssemblyName>
		<UseWinUI>true</UseWinUI>
		<PlatformTarget>x64</PlatformTarget>
		<Platforms>AnyCPU;x64;ARM64</Platforms>

		<ErrorOnDuplicatePublishOutputFiles>false</ErrorOnDuplicatePublishOutputFiles>		
	</PropertyGroup>

	<PropertyGroup>
		<!--<CsWinRTIncludes>Microsoft.Devices.Midi2</CsWinRTIncludes>-->
		<CsWinRTGeneratedFilesDir>$(OutDir)</CsWinRTGeneratedFilesDir>
		<!--<CsWinRTExcludes></CsWinRTExcludes> -->
		<!--<CsWinRTExcludesPrivate></CsWinRTExcludesPrivate> -->
	</PropertyGroup>


	<PropertyGroup>
		<RestoreSources>
			https://api.nuget.org/v3/index.json;
			../../../app-dev-sdk/sdk-projections/MidiAppSdkCSharpProjection/nuget
		</RestoreSources>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.Devices.Midi2" Version="0.1.0-prerelease" />
	</ItemGroup>
	
	<ItemGroup>
		<PackageReference Include="Microsoft.UI.Xaml" Version="2.8.4" />
		<PackageReference Include="Microsoft.Windows.CsWinRT" Version="2.0.3" />
  </ItemGroup>

</Project>
