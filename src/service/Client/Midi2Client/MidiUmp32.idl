MIDI_IDL_IMPORT

// wraps the UMP type so we still quickly read/write packed data appropriately

import "IMidiUmp.idl";

namespace Windows.Devices.Midi2
{
	[MIDI_API_CONTRACT(1)]
	unsealed runtimeclass MidiUmp32 : IMidiUmp
	{
		MidiUmp32();
		MidiUmp32(UInt64 timestamp, UInt32 word0);

		UInt32 Word0{ get; set; };
	};
}